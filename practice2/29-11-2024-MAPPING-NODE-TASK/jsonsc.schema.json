{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "InputData": {
      "type": "object",
      "properties": {
        "Name": {
          "type": "string"
        },
        "Id": {
          "type": "string"
        },
        "Date": {
          "type": "string",
          "format": "date"
        },
        "status": {
          "type": "string"
        }
      },
      "required": ["Name", "Id", "Date", "status"]
    }
  },
  "required": ["InputData"]
}

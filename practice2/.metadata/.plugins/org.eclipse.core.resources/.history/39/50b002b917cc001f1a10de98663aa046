

CREATE DATABASE MODULE database_input_node_Database
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		DECLARE inref REFERENCE TO Root.XMLNSC.Employee;
		
		INSERT INTO Database.my.DATA_INSERT_TABLE (ID, NAME, ADDRESS ) VALUES (inref.ID, inref.Name, inref.Address);
		
		RETURN TRUE;
	END;
		
END MODULE;

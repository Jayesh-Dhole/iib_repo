

CREATE COMPUTE MODULE InsertData_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		CALL CopyEntireMessage();
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot.JSON.Data.Result.EMP_ID =	InputRoot.DataObject.ns:EMP_EVENT.EMP_ID;
		SET OutputRoot.JSON.Data.Result.EVENTID =	InputRoot.DataObject.ns:EMP_EVENT.EVENTID;
		SET OutputRoot.JSON.Data.Result.DESC =	InputRoot.DataObject.ns:EMP_EVENT.EVENT_DESC;
	END;
END MODULE;

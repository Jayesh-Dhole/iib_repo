CREATE COMPUTE MODULE STORED_PROCEDURES_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		CALL CopyEntireMessage();
		DECLARE in1 REFERENCE TO
		CALL Insertion();
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		CREATE PROCEDURE Insertion (IN id CHARACTER, IN name CHARACTER,IN marks CHARACTER )
		LANGUAGE DATABASE
		EXTERNAL NAME "INSERTDATA";
	END;
END MODULE;
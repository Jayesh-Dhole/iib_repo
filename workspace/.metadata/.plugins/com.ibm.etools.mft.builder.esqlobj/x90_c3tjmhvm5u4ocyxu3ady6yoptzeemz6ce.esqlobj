/*EATE FUNCTION Main() RETURNS BOOLE*/
	BEGIN
		CALL CopyEntireMessage();
		DECLARE in1 REFERENCE TO InputRoot.XMLNSC.Proc;
		call Gammat(in1.ID,in1.NAME,in1.SALARY,in1.AGE);
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		CREATE PROCEDURE Gammat(IN ID INTEGER, IN NAME CHARACTER, IN SALARY INTEGER, IN AGE INTEGER)
		LANGUAGE DATABASE
		EXTERNAL NAME "TEMP";
	END;
